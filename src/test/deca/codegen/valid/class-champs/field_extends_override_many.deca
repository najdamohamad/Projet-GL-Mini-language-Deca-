class A {
    int x = 1;
    int y = 2;
}
class B extends A {
    int y = 3;
}
class C extends B {
    int z = 4;
}
class D extends C {
    int y = 5;
    int m = 6;
}

{
    A a = new A();
    B b = new B();
    C c = new C();
    D d = new D();
    println(a.x, a.y);
    println(b.x, b.y);
    println(c.x, c.y, c.z);
    println(d.x, d.y, d.z, d.m);
}